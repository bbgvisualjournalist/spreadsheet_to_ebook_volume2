<?xml version='1.0' encoding='utf-8'?>
<package xmlns="http://www.idpf.org/2007/opf" unique-identifier="uuid_id" version="3.0">
<metadata xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:opf="http://www.idpf.org/2007/opf" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:calibre="http://calibre.kovidgoyal.net/2009/metadata" xmlns:dc="http://purl.org/dc/elements/1.1/">
	<dc:creator id="author"><%= global.book.config[0].authorenglish %></dc:creator>
	<meta refines="#author" property="alternate-script" xml:lang="<%= global.book.config[0].languagecode %>"><%= global.book.config[0].author %></meta>
	<dc:publisher><%= global.book.config[0].publisher %></dc:publisher>
	<dc:date><%= pubDate %></dc:date>
	<dc:description id="description"><%= global.book.meta[book].description %> </dc:description>
	<meta refines="#description" property="alternate-script" xml:lang="en"><%= global.book.meta[book].descriptionenglish %> </meta>
	<dc:language>zh</dc:language>
	<dc:identifier id="uuid_id" ><%= global.book.meta[book].uuid %> </dc:identifier>
	<dc:identifier id="pub-identifier" >urn:isbn:<%= global.book.meta[book].isbn %></dc:identifier>
	<dc:title id="bookTitle"><%= global.book.meta[book].booktitle %></dc:title>
	<meta refines="#bookTitle" property="alternate-script" xml:lang="en"><%= global.book.meta[book].booktitleenglish %></meta>
	<dc:subject>國家禁區</dc:subject>
	<dc:subject>Guójiā jìnqū</dc:subject>
	<dc:subject><%= global.book.config[0].author %></dc:subject>
	<dc:subject><%= global.book.config[0].authorenglish %></dc:subject>
	<meta name="cover" content="cover"/>
	<meta property="dcterms:modified"><%= date %></meta>
</metadata>
<manifest>
	<item id="cover" href="cover.xhtml" media-type="application/xhtml+xml"/>
	<item id="toc" href="toc.xhtml" media-type="application/xhtml+xml" properties="nav"/>
	<item id="ncx" href="toc.ncx" media-type="application/x-dtbncx+xml"/>
	<item id="titlepage" href="titlepage.xhtml" media-type="application/xhtml+xml"/>
	<item id="intro" href="introduction.xhtml" media-type="application/xhtml+xml"/>
	<item id="body" href="bodymatter.xhtml" media-type="application/xhtml+xml"/>
	<item id="back" href="backmatter.xhtml" media-type="application/xhtml+xml"/>
	<item id="page_css" href="stylesheets/page_styles.css" media-type="text/css"/>
	<item id="css" href="stylesheets/stylesheet.css" media-type="text/css"/>
	<item id="font1" href="fonts/AdobeHeitiStd-Regular.otf" media-type="application/vnd.ms-opentype"/>
	<item id="font2" href="fonts/AdobeFanHeitiStd-Bold.otf" media-type="application/vnd.ms-opentype"/>
	<item id="font3" href="fonts/ヒラギノ明朝ProW6.otf" media-type="application/vnd.ms-opentype"/>
	<item id="font4" href="fonts/ヒラギノ明朝ProW3.otf" media-type="application/vnd.ms-opentype"/>
		
	<item id="cover-image" href="images/v2_<%= photos_bookNumber[i] %>/<%= photos[2] %>" media-type="image/jpeg" properties="cover-image"/>
	
	<item id="intro-image1" href="images/v2_<%= photos_bookNumber[i] %>/<%= photos[0] %>" media-type="image/jpeg"/>
	<item id="intro-image2" href="images/v2_<%= photos_bookNumber[i] %>/<%= photos[1] %>" media-type="image/jpeg"/>
	
	<%for (var i=3; i<photos.length;i++){ %>
		<item id="photo<%= i-3 %>" href="images/v2_<%= photos_bookNumber[i] %>/<%= photos[i] %>" media-type="image/jpeg"/>
	<% } %>
</manifest>
<spine toc="ncx">
	<itemref idref="cover"/>
	<itemref idref="titlepage"/>
	<itemref idref="toc"/>
	<itemref idref="intro"/>
	<itemref idref="body"/>
	<itemref idref="back"/>
</spine>
</package>
